        <string id="category_Localization">Localization</string>
        <string id="name_DefaultLocale">Default document language (Western)</string>
        <string id="desc_DefaultLocale">Specifies the default document language - Western.</string>
        <string id="name_DefaultLocale_CJK">Default document language (CJK)</string>
        <string id="desc_DefaultLocale_CJK">Specifies the default document language - Chinese, Japanese, Korean.</string>
        <string id="name_DefaultLocale_CTL">Default document language (CTL)</string>
        <string id="desc_DefaultLocale_CTL">Specifies the default document language - complex text layout (e.g. Arabic, Indian etc., scripts).</string>
        <string id="name_UILocale">User interface language</string>
        <string id="desc_UILocale">Specifies the user interface (UI) language. An empty string means: use the desktop's UI language if possible.</string>
        <string id="name_Locale">Locale</string>
        <string id="desc_Locale">Contains the user's desktop locale setting.</string>
        <string id="name_ooSetupCurrency">Currency</string>
        <string id="desc_ooSetupCurrency">Indicates the currency settings defined by the user. By default, this label is empty and the currency defined by system locale.</string>
        <string id="name_DecimalSeparatorAsLocale">Decimal separator as locale</string>
        <string id="desc_DecimalSeparatorAsLocale">Indicates that the decimal separator (dot or comma) is used as appropriate for the selected locale instead of the one related to the default keyboard layout.</string>
        <string id="name_IgnoreLanguageChange">Ignore language change</string>
        <string id="desc_IgnoreLanguageChange">Indicates whether changes to the system input language/keyboard will be ignored. If ignored, when new text is typed that text will follow the language of the document or current paragraph, not the current system language.</string>

        <string id="category_Security">Security</string>
        <string id="name_RemovePersonalInfoOnSaving">Remove personal information on saving</string>
        <string id="desc_RemovePersonalInfoOnSaving">Specifies whether to remove personal information on saving.

Default value: Do not remove</string>
        <string id="name_RecommendPasswordProtection">Recommend password protection on saving</string>
        <string id="desc_RecommendPasswordProtection">Specifies whether to recommend password protection when saving documents.

Default value: Do not recommend</string>
        <string id="name_HyperlinksWithCtrlClick">Ctrl-click required to follow hyperlinks</string>
        <string id="desc_HyperlinksWithCtrlClick">Specifies whether Ctrl-click is required to follow hyperlinks.
Default value: Ctrl-click</string>
        <string id="name_WarnSaveOrSendDoc">Security warnings when saving or sending</string>
        <string id="desc_WarnSaveOrSendDoc">Specifies whether to warn when saving or sending documents with personal/hidden data.

Default value: Do not warn</string>
        <string id="name_WarnSignDoc">Security warnings when signing</string>
        <string id="desc_WarnSignDoc">Specifies whether to warn when signing documents with personal/hidden data.

Default value: Warn</string>
        <string id="name_WarnPrintDoc">Security warnings when printing</string>
        <string id="desc_WarnPrintDoc">Specifies whether to warn when printing documents with personal/hidden data.

Default value: Do not warn</string>
        <string id="name_WarnCreatePDF">Security warnings when creating PDF</string>
        <string id="desc_WarnCreatePDF">Specifies whether to warn when creating PDF documents with personal/hidden data.

Default value: Do not warn</string>
        <string id="name_MacroSecurityLevel">Macro security level</string>
        <string id="desc_MacroSecurityLevel">Level of Macro security. Range 0 (lowest) - 3 (highest).

- Low (not recommended). All macros will be executed without confirmation. Use this setting only if you are certain that all documents that will be opened are safe.
- Medium. Confirmation required before executing macros from untrusted sources.
- High. Only signed macros from trusted sources are allowed to run. Unsigned macros are disabled.
- Very high. Only macros from trusted file locations are allowed to run. All other macros, regardless whether signed or not, are disabled.

Default value: High</string>
        <string id="low">Low (not recommended)</string>
        <string id="medium">Medium</string>
        <string id="high">High</string>
        <string id="veryhigh">Very high</string>
        <string id="name_DisableMacrosExecution">Disable macro execution</string>
        <string id="desc_DisableMacrosExecution">Specifies whether the macro execution is disabled in general. If it is set to true, the MacroSecurityLevel is ignored. If it is set to false, the mentioned entry specifies the level of macro security.

Default value: Macros enabled</string>
