        &quot;last_updated_millis&quot;: &quot;1000000&quot;, 
        &quot;ends_at&quot;: {
          &quot;hour&quot;: 7, 
          &quot;minute&quot;: 30
        }
      }
    ]
  }, 
  &quot;time_usage_limit&quot;: {
    &quot;monday&quot;: {
      &quot;usage_quota_mins&quot;: 120, 
      &quot;last_updated_millis&quot;: &quot;1200000&quot;
    }, 
