      <string id="WindowsComponents">Componentes de Windows</string>
      <string id="VistaOnly">Microsoft Windows Vista o versión posterior</string>
      <string id="VistaOrRedist">Microsoft Windows Vista, o cualquier versión de Windows con Windows Search 3.01 o posterior</string>
      <string id="RedistOnly">Microsoft Windows XP, o Windows Server 2003 con la versión Windows Search 3.01 o posterior</string>
