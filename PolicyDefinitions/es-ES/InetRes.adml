      <string id="Advanced_ProxyHttp1_1">Usar HTTP 1.1 a través de conexiones proxy</string>
      <string id="Advanced_EnableSSL3Fallback">Permitir reserva para SSL 3.0 (Internet Explorer)</string>
      <string id="Advanced_FallbackNonProtected">Sitios en modo no protegido</string>
      <string id="Advanced_FallbackAll">Todos los sitios</string>
      <string id="Advanced_FallbackNone">No hay sitios</string>
