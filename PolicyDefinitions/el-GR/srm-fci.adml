        <multiTextBox refId="EmailMessageText"/>
        <text> </text>
        <text>Παραλήπτες μηνύματος ηλεκτρονικού ταχυδρομείου</text>
        <checkBox refId="PutDataOwnerOnToCheck" defaultChecked="true">Κάτοχος φακέλου</checkBox>
        <checkBox refId="PutAdminOnToCheck" defaultChecked="true">Διαχειριστής διακομιστή αρχείων</checkBox>
        <textBox refId="AdditonalEmailToText">
          <label>Πρόσθετοι παραλήπτες:</label>
        </textBox>
